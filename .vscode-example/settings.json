{
    "phpunit.command": "docker-compose -f .docker/docker-compose.yml run --rm server",
    "phpunit.phpunit": "/app/vendor/bin/phpunit",
    "phpunit.paths": {
        "${workspaceFolder}": "/app"
    },
    "phpunit.args": [
        "--configuration",
        "/app/phpunit.xml"
    ],
    "php.suggest.basic": false,
}